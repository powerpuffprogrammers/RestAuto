package loggingIn;

import javax.swing.JFrame;

public class TabletApp {
	public static void main(String[] args) {
		
		//GUI stuff
		JFrame frame= new JFrame("SWE");
		frame.setVisible(true);
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		LogInScreen pan1= new LogInScreen();
		frame.setContentPane(pan1);
		frame.pack();
		while(true){
			while('0'==pan1.loggedIn){}
			//calls the interface to set up the screen
			if(pan1.loggedIn=='h'){
				
			}
			else if(pan1.loggedIn=='c'){
				
			}
			else if(pan1.loggedIn=='m'){
				
			}
			else if(pan1.loggedIn=='w'){
				
			}
			//show screen 1 again
			pan1.loggedIn='0';
			frame.setContentPane(pan1);
		}
		
		
	}
}
